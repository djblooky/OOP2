<Window x:Class="WpfCurrencyMidterm.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfCurrencyMidterm"
        mc:Ignorable="d"
        Title="MainWindow" Height="300" Width="300">
    <Grid Margin="50,150,50,50">
        <Button x:Name="ButtonMakeChange" Click="ButtonMakeChange_Click">Make Change</Button>
        <Button x:Name="ButtonCurrencyRepo" Click="ButtonRepo_Click" Margin="0,-150,0,150">Currency Repo</Button>
    </Grid>
</Window>
