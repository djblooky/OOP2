<Window x:Class="WpfAppWithObject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppWithObject"
        mc:Ignorable="d"
        Title="Welcome to Devil Dawgs" Height="300" Width="300">
    <Grid>
        <StackPanel>
            <Label>Select a menu item:</Label>
            <Button Content="Dawg" Click="Dawg_Button_Click"></Button>
            <Button Content="Burger" Click="Burger_Button_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
